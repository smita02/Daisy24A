package collectiondemo.hashmapdemo;

import java.util.ArrayList;

public class TwoArrylist {

	public static void main(String[] args) {
	ArrayList al = new ArrayList();
	al.add(10);
	al.add(11);
	al.add(12);
	al.add(13);
//	al.add(14);
	
	ArrayList ax = new ArrayList();
	ax.add(10);
	ax.add(11);
	ax.add(12);
	ax.add(13);
	ax.add(40);
//	ax.add(50);
	
	System.out.print(al.containsAll(ax));
	System.out.print(ax.containsAll(al));
	//ax.contains(al);

	}

}
